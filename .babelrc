{
  "presets": [
    "react",
    "env",
    "stage-2"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "img": "./resources/assets/img",
          "components": "./resources/assets/js/components",
          "~redux": "./resources/assets/js/redux",
          "views": "./resources/assets/js/views",
          "utils": "./resources/assets/js/utils"
        }
      }
    ]
  ]
}
